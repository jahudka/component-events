{
    "name": "jahudka/component-events",
    "type": "library",
    "license": "MIT",
    "description": "Relay events to lazily created components",
    "homepage": "https://github.com/jahudka/component-events",
    "authors": [
        {"name": "Dan Kadera", "email": "me@subsonic.cz"}
    ],
    "require": {
        "php": ">=7.1",
        "nette/component-model": "^2.4 || ^3.0",
        "nette/di": "^2.4 || ^v3.0",
        "nette/application": "^2.4 || ^3.0"
    },
    "require-dev": {
        "doctrine/event-manager": "^1.0",
        "symfony/event-dispatcher": "^3.4 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Jahudka\\ComponentEvents\\": "src/"
        },
        "files": [
            "src/compatibility.php"
        ]
    }
}
